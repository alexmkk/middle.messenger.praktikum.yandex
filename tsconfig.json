{
  "ts-node": {
    "files": true
  },
  "compilerOptions": {
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "lib": ["esnext", "dom"],
    "target": "es5",
    "esModuleInterop": true,
    "module": "commonjs",
    "forceConsistentCasingInFileNames": true
  }
}