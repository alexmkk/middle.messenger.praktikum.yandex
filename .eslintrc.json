{
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended",
    "prettier/prettier",
    "prettier"
  ],
  "rules": {
    "max-len": [1, 100],
    "max-params": [2, 3],
    "@typescript-eslint/no-non-null-assertion": "off"
  }
}